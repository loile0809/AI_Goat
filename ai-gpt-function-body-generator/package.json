{
  "name": "ai-gpt-function-body-generator",
  "publisher": "dzhoang",
  "displayName": "AI GPT â€“ Generate Function Body",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  "onCommand:ai-gpt.generateFunctionBody"
],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-gpt-function-body-generator.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ai-gpt.generateFunctionBody",
        "title": "Generate Function Body"
      }
    ],
    "configuration": {
      "title": "AI GPT Function Body Generator",
      "properties": {
        "aiGpt.apiBaseUrl": {
          "type": "string",
          "default": "http://127.0.0.1:8000",
          "description": "Base URL of FastAPI /v1/solve service"
        },
        "aiGpt.mode": {
          "type": "string",
          "enum": [
            "fast",
            "quality"
          ],
          "default": "fast",
          "description": "Default generation mode"
        },
        "aiGpt.requestTimeoutMs": {
          "type": "number",
          "default": 60000,
          "description": "Request timeout in milliseconds"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
